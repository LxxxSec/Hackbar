{
   "background": {
      "scripts": [ "scripts/background.js" ]
   },
   "commands": {
      "execute_url": {
         "description": "Trigger 'Execute'",
         "suggested_key": {
            "default": "Alt+X",
            "mac": "MacCtrl+X"
         }
      },
      "load_url": {
         "description": "Trigger 'Load'",
         "suggested_key": {
            "default": "Alt+A",
            "mac": "MacCtrl+A"
         }
      },
      "split_url": {
         "description": "Trigger 'Split'",
         "suggested_key": {
            "default": "Alt+S",
            "mac": "MacCtrl+S"
         }
      }
   },
   "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
   "description": "HackBar for Chrome",
   "devtools_page": "devtools.html",
   "icons": {
      "128": "images/icon.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "HackBar",
   "permissions": [ "storage", "webRequest", "webRequestBlocking", "\u003Call_urls>" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.5.0",
   "web_accessible_resources": [ "payloads/*" ]
}
